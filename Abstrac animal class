abstract class Animal {
    public abstract void sound();
}

class Lion extends Animal {
    @Override
    public void sound() {
        System.out.println("The lion can roar");
    }
}

class Tiger extends Animal {
    @Override
    public void sound() {
        System.out.println("The tiger can growl");
    }
}

public class Main {
    public static void main(String[] args) {
        Lion lion = new Lion();
        lion.sound();

        Tiger tiger = new Tiger();
        tiger.sound();
    }
}
